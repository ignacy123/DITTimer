<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.StatsWindow">
   <children>
      <LineChart fx:id="chart" layoutX="51.0" layoutY="57.0" prefHeight="286.0" prefWidth="419.0" title="lokalne">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
         <effect>
            <SepiaTone />
         </effect>
      </LineChart>
      <TextField fx:id="countField" layoutX="415.0" layoutY="32.0" prefHeight="16.0" prefWidth="29.0" promptText="set count" style="-fx-background-color: aqua;" text="10" />
      <Button fx:id="updateButton" layoutX="444.0" layoutY="32.0" mnemonicParsing="false" onAction="#updateCount" prefHeight="16.0" prefWidth="48.0" text="count" />
   </children>
</AnchorPane>
